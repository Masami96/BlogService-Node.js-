<div>
  <%-username%>'s Posts
  <ul>
    <% for(let i = pid; i < pid + 5; i++) { %>
      <% if (titles[i] != null || bodies[i] != null) { %>
        <li>Title:<%- titles[i] %>
          Body:<%- bodies[i] %></li>
      <% } %>
    <% } %>
  </ul>
  <% if(titles.length > 5) { %>
    <a id="next" href="/blog/<%-username%>?curid=<%-nextid%>">Next Posts</a>
  <% } %>
</div>
